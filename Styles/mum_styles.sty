\newcommand{\gradqhj }{\mbox{$\nabla^2 \! h_j$}} 
\newcommand{\gradqvph}{\mbox{$\nabla^2 \! \varphi$}} 
\newcommand{\gradqvphi}{\mbox{$\nabla^2 \! \varphi^{(i)} $}} 
